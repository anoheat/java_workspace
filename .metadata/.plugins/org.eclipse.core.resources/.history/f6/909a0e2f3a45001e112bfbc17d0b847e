package day09;

public class Pra {

	public static void main(String[] args) {
		int[] a = new int[3];
		for (int i = 0; i < 3; i++) {
			a[i] = (int) (Math.random() * 9) + 1;
			System.out.println(i+"dd "+a[i]);
			for (int j = 0; j < i; j++) {
				if (a[j] == a[i]) {
					i--;
					break;
				}
				System.out.println(i+"... "+a[i]);
			}
			
		}
	

	}


	
	
}
